<td>
  <%= req.id %>
</td>
<td>
  <%= req.status %>
</td>
<td>
  <%= req.start_date %>
</td>
<td>
  <%= req.end_date %>
</td>
<td>
  <%= User.find(req.user_id).username %>
</td>
  <% if req.pending? %>
    <td>
      <%= render "approve", req: req %>
    </td>
    <td>
      <%= render "deny", req: req %>
    </td>
  <% end %>
  </form>
</td>
