<h2>Sign Up</h2>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>

  <label>
    Username
    <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>

<br>
  <label>
    Password
    <input type="text" name="user[password]">
  </label>
<br>

  <button>Create User</button>
</form>
